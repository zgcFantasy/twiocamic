{
  "name": "twiocamic",
  "version": "0.1.1",
  "description": "A common React components library",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zgcFantasy/twiocamic"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1",
    "web-vitals": "^1.1.0"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build-all": "npm run build-ts && npm run build-css",
    "test": "react-scripts test",
    "test-nowatch": "cross-env CI=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "node-sass ./src/styles/index.scss ./dist/index.css",
    "lint": "eslint --ext js,ts,tsx src --max-warnings 5",
    "prepublish": "npm run test-nowatch && npm run lint && npm run build-all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-nowatch && npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "",
  "devDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.2.2",
    "react-scripts": "4.0.3",
    "node-sass": "^5.0.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/node-logger": "^6.1.21",
    "@storybook/preset-create-react-app": "^3.1.6",
    "@storybook/react": "^6.1.21",
    "@types/storybook__addon-info": "^5.2.3",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-transition-group": "^4.4.1",
    "cross-env": "^7.0.3"
  }
}